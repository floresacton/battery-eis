    "Vi = 10\n",
    "r1 = 1\n",
    "r2 = 1\n",
    "c = 999999\n",
    "\n",
    "P = 9\n",
    "Q = 0\n",
    "\n",
    "Vo = (Vi - Q/c + np.sqrt((Q/c - Vi)**2 - 4*P*r1)) / 2\n",
    "\n",
    "Ic = Vi/r1 + (Q/c + Vo)*(-1/r1 - 1/r2) + r1/r2 * P / (Vi - (Q/c + Vo))\n",
    "\n",
    "dQ = Ic * 1\n",
    "\n",
    "print(Vo)\n",
    "print(Ic)\n",
    "print(dQ)\n"
